{
  "name": "example-prisma",
  "version": "1.0.0",
  "description": "Example application showing prismajs orm and schema management capabilities.",
  "main": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "db:seed": "node ./db/scripts/seed.js",
    "db:clear": "node ./db/scripts/clear.js",
    "db:backup": "node ./db/scripts/backup.js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gvanastasov/example-prisma.git"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gvanastasov/example-prisma/issues"
  },
  "homepage": "https://github.com/gvanastasov/example-prisma#readme",
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@prisma/client": "^4.15.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0"
  }
}
